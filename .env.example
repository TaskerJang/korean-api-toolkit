# API Base URLs
UPBIT_API_URL=https://api.upbit.com
KIS_API_URL=https://openapi.koreainvestment.com:9443
KAKAO_API_URL=https://dapi.kakao.com
TMAP_API_URL=https://apis.openapi.sk.com

# 업비트 API (인증 불필요 - 공개 API)
# UPBIT_ACCESS_KEY=your_access_key_here
# UPBIT_SECRET_KEY=your_secret_key_here

# 한국투자증권 API
KIS_APP_KEY=your_app_key_here
KIS_APP_SECRET=your_app_secret_here
KIS_ACCOUNT_NUMBER=your_account_number_here

# 카카오 로컬 API
KAKAO_REST_API_KEY=your_rest_api_key_here

# T맵 API
TMAP_APP_KEY=your_app_key_here

# 개발/운영 환경 구분
ENVIRONMENT=development

# 로그 레벨
LOG_LEVEL=INFO

# API 요청 타임아웃 (초)
REQUEST_TIMEOUT=30

# 재시도 횟수
MAX_RETRIES=3